{
  "name": "Stegman",
  "description": "Steganographic messaging",
  "version": "1.0.7",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "optional_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://blank.org/*"
      ],
      "run_at": "document_end",
      "js": [
        "content/index.js"
      ],
      "css": [
        "./content/index.css"
      ]
    }
  ],
  "background": {
    "page": "background/index.html",
    "persistent": false
  },
  "icons": {
    "128": "logo128.png"
  },
  "version_name": "1.0.7"
}
